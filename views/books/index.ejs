<h2>Search Author</h2>
<form action="/books" method = "GET">
    <div></div>
        <label for="title">Title </label>
        <input type="text" name="title" id="title" value="<%=searchOption.title%>">
    </div>
    <div>
        <label for="publishAfter">Publish After</label>
        <input type="date" name="publishAfter" id="publishAfter">
        
        <label for="publishBefore">Publish Before</label>
        <input type="date" name="publishBefore" id="publishBefore">
    </div>

    <button type="submit">Search</button>
</form>
<br>
</div>
<%books.forEach(book => { %>
    <img src="<%= book.coverImagePath %>"  height="150" width="100">
<% }) %>
